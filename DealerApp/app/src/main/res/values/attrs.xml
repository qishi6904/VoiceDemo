<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--for demo start-->
    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference" />
        <attr name="msv_emptyView" format="reference" />
        <attr name="msv_errorView" format="reference" />
        <attr name="msv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
        </attr>
    </declare-styleable>
    <!--for demo end-->

    <declare-styleable name="triangleView">
        <attr name="triangle_view_color" format="color" />
        <attr name="triangle_view_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ringChartView">
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="line_width" format="dimension" />
        <attr name="indicator_width" format="dimension" />
        <attr name="center_text_size" format="dimension" />
        <attr name="center_text_color" format="color" />
    </declare-styleable>

</resources>